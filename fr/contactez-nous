    <script type="text/javascript">

        $(function() {

            $('#form-contact').validate({
                errorElement: 'span',
                rules: {email: {email: true} },
                messages: {
                    email: {
                        required:   'obligatoire',
                        email:      'obligatoire'
                    },
                    name:         'obligatoire',
                    subject:      'obligatoire',
                    message:      'obligatoire',
                    agreement:      'obligatoire'
                },
                errorPlacement: function(error,element) {
                    error.appendTo( element.prev('label') );
                    // error dla checkbox
                    if (element.attr('name') == 'agreement') error.appendTo( element.parent('label') );
                    // error dla pozostalych pol
                    else error.appendTo( element.prev('label') );
                },
                submitHandler: function(form) {
                    $('.msg').html('<p>sending...</p>');
                    jQuery.ajax({
                        type:'POST',
                        dataType:'json',
                        data: jQuery('#form-contact').serialize(),
                        success: function(data, textStatus){
                            if(data.wynik) {
                                $('#form-contact').hide();
                                $('.msg').html(data.msg);
                            } else {
                                $('.msg').html(data.msg);
                            }
                        },
                        error: function() {
                            $('.msg').html('Error');
                        },
                        url: '/fr/ajx-contact'
                    });
                }
            });

        });
    </script>


