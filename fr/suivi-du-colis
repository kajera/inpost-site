    <script>
        $(document).ready(function(){
            $.validator.addMethod("regex",function (value, element) {
                   var re = new RegExp('^[0-9a-zA-Z]*$');
                      return this.optional(element) || re.test(value);
                  },
                  "Only alphanumeric values allowed"
          );
            $('#form-find-parcel').validate({
                errorElement: 'span',
                rules:{
                    "find_parcel[parcel_number]" : {
                        required : true,
                        regex: true
                    }
                },
                messages:{
                    "find_parcel[parcel_number]" :{
                        required: "obligatoire"
                    }
                }
            });

            //creating querystring
                                                    var link = '/fr/suivi-du-colis';
            $("#find_parcel_submit").click(function () {
                if ($("#form-find-parcel").valid()) {
                    parcels = $('#find_parcel_parcel_number').val();
                    link = link + '?parcel=' + parcels;
                    $("#form-find-parcel").prop('action', link);
                }
            })

        });

    </script>


