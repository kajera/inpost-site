            $(function() {
                $('.popupbox a.pop').mouseover(function () {
                    $(this).next('div.hide').show();
                });
                $('.popupbox div.hide').mouseover(function () {
                    $(this).show();
                });
                $('.popupbox a.pop').mouseout(function () {
                    $('.popupbox div.hide').hide();
                });
                $('.popupbox div.hide').mouseout(function () {
                    $('.popupbox div.hide').hide();
                });
            });
        </script>
    
    



                                                                            

