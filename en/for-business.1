            <h2>E-mail us</h2>
            <form id="form-contact" method="post">

                                    <input type="hidden" name="recipients" id="recipients" value="sales@inpost24.ca">
                                <input type="hidden" name="formname" id="formname" value="contact">

                <fieldset>

                    <label class="span4" for="name">name:</label>
                    <input class="span4 required" type="text" name="name" id="name" placeholder="your name" />
                    <label class="span4" for="email">e-mail:</label>
                    <input class="span4 required" type="text" name="email" id="email" placeholder="email" />
                    <label class="span4" for="subject">subject:</label>
                    <select class="span4 required" id="subject" name="subject">
                        <option value="">--- choose ---</option>
                                                    <option value="How to collect/dispatch a parcel?">How to collect/dispatch a parcel?</option>
                                                    <option value="Status of a parcel">Status of a parcel</option>
                                                    <option value="Parcel Machine is out of order">Parcel Machine is out of order</option>
                                                    <option value="General information about Parcel Machines">General information about Parcel Machines</option>
                                                    <option value="Integration">Integration</option>
                                                    <option value="New location">New location</option>
                                                    <option value="Website comments">Website comments</option>
                                                    <option value="Other subject">Other subject</option>
                                            </select><label for="message">message:</label>
                    <textarea rows="5" class="span6 required" name="message" id="message"></textarea>

                    <p class="smaller">The personal data provided will be processed by InPost Canada Inc. (55 Browns Line, Toronto ON) in order to fulfill your request. You will have full access rights to your data and may make corrections. Providing this data is voluntary, but necessary in order to process the transaction.</p>

                    <label class="checkbox">
                        <input type="checkbox" name="agreement" id="agreement" value="1" class="required" />I would like to be contacted by InPost Canada Inc. regarding this inquiry.                    </label>
                    <label class="checkbox">
                        <input type="checkbox" name="agreementSmanago" id="agreementSmanago" value="1"  />I would like to receive communications regarding special offers or any other information from InPost.                    </label>
                    <input type="submit" class="btn btn-primary btn-large" value="send" />
                </fieldset>

            </form>
            <div class="msg"></div>



    <script type="text/javascript">

        $(function() {

            $('#form-contact').validate({
                errorElement: 'span',
                rules: {email: {email: true} },
                messages: {
                    email: {
                        required:   'required',
                        email:      'required'
                    },
                    name:         'required',
                    subject:      'required',
                    message:      'required',
                    agreement:      'required'
                },
                errorPlacement: function(error,element) {
                    error.appendTo( element.prev('label') );
                    // error dla checkbox
                    if (element.attr('name') == 'agreement') error.appendTo( element.parent('label') );
                    // error dla pozostalych pol
                    else error.appendTo( element.prev('label') );
                },
                submitHandler: function(form) {
                    $('.msg').html('<p>sending...</p>');
                    jQuery.ajax({
                        type:'POST',
                        dataType:'json',
                        data: jQuery('#form-contact').serialize(),
                        success: function(data, textStatus){
                            if(data.wynik) {
                                $('#form-contact').hide();
                                $('.msg').html(data.msg);
                            } else {
                                $('.msg').html(data.msg);
                            }
                        },
                        error: function() {
                            $('.msg').html('Error');
                        },
                        url: '/en/ajx-contact'
                    });
                }
            });

        });
    </script>


